<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 5.2.0//EN"
        "http://www.liferay.com/dtd/liferay-portlet-app_5_2_0.dtd">

<liferay-portlet-app>
    <portlet>
        <portlet-name>library</portlet-name>
        <instanceable>true</instanceable>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>

    <portlet>
        <portlet-name>ccc-ajax-portlet</portlet-name>
        <icon>/images/icon.png</icon>
        <instanceable>true</instanceable>
        <css-class-wrapper>ccc-ajax-portlet-portlet</css-class-wrapper>
    </portlet>

    <portlet>
        <portlet-name>menu</portlet-name>
        <instanceable>true</instanceable>
        <header-portlet-css>/css/menu.css</header-portlet-css>
    </portlet>
</liferay-portlet-app>